<h1>Pages</h1>
<%= link_to 'New page', new_admin_page_path %>
<table>
  <tr>
    <th>Title</th>
    <th>Page</th>
  </tr>
  <% @pages.each do |page| %>
    <tr>
      <td><%= page.title %></td>
      <td>
        <%= link_to 'See', admin_page_path(page) %>
        <%= link_to 'Edit', edit_admin_page_path(page) %>
        <%= link_to 'Delete', admin_page_path(page), method: :delete %>
      </td>
    </tr>
  <% end %>
</table>
